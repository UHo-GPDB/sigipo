{
  "name": "gpdb",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "author": "gpdb",
  "license": "private",
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "start_django": "python manage.py runserver"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ]
    },
    "ignorePatterns": [
      "highcharts.min.js",
      "highchart-map.min.js",
      "highchart-maps-exporting.min.js",
      "bootstrap.bundle.min.js",
      "jquery-3.6.0.min.js",
      "select2.min.js",
      "select2-focus-fix.js"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "Highcharts": true,
      "bootstrap": true,
      "jQuery": true,
      "$": true,
      "django": "readonly",
      "Chart": "readonly",
      "L": true
    },
    "parserOptions": {
      "ecmaVersion": 2022
    }
  },
  "devDependencies": {
    "@types/node": "^15.12.5"
  }
}
